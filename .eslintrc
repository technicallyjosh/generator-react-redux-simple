{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "mocha": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true,
            "generators": true
        }
    },
    "rules": {
        "object-curly-spacing": [2, "always"],
        "one-var": [2, "never"],
        "no-array-constructor": 2,
        "no-console": 1,
        "no-const-assign": 2,
        "no-new-object": 2,
        "no-unused-vars": 1,
        "no-var": 2,
        "prefer-arrow-callback": 2,
        "prefer-const": 2,
        "prefer-spread": 1,
        "prefer-template": 2,
        "quote-props": [2, "as-needed"],
        "quotes": [2, "single"],
        "require-yield": 1,
        "semi": 2,
        "keyword-spacing": 2,
        "no-debugger": 1,
        "react/no-render-return-value": 0
    },
    "plugins": []
}
